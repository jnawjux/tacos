   "source": [
    "taco_recipe =  {'ground beef, lb': .5, 'taco shells': 6, 'lettuce, oz.': 4, 'cheese, oz.': 4, 'tomatoes, oz.': 4}\n",
    "\n",
    "def make_taco_dinner(peeps):\n",
    "    print(\"Go buy:\")\n",
    "    for t, i in taco_recipe.items():\n",
    "        print(t + \": \" + str(i * peeps))\n",
    "\n"
   ]
